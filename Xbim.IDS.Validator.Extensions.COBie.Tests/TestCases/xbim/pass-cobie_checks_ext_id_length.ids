<?xml version="1.0" encoding="utf-8"?>
<ids xsi:schemaLocation="http://standards.buildingsmart.org/IDS http://standards.buildingsmart.org/IDS/0.9.7/ids.xsd" xmlns="http://standards.buildingsmart.org/IDS" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <info>
    <title>A Cobie XLS model can be tested</title>
  </info>
  <specifications>
    <specification name="All components should have a ifcGuid" ifcVersion="COBIE2.4">
      <applicability minOccurs="1" maxOccurs="unbounded">
        <entity>
          <name>
            <xs:restriction base="xs:string">
              <xs:enumeration value="COBIEFACILITY" />
              <xs:enumeration value="COBIEFLOOR" />
              <xs:enumeration value="COBIESPACE" />
              <xs:enumeration value="COBIETYPE" />
              <xs:enumeration value="COBIECOMPONENT" />
            </xs:restriction>
          </name>
        </entity>
      </applicability>
      <requirements>
        <attribute cardinality="required">
          <name>
            <simpleValue>ExternalId</simpleValue>
          </name>
          <value>
            <xs:restriction base="xs:string">
              <xs:length value="22" fixed="false" />
            </xs:restriction>
          </value>
        </attribute>
      </requirements>
    </specification>
  </specifications>
</ids>